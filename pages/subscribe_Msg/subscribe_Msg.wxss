/* pages/subscribe_Msg/subscribe_Msg.wxss */
.container{
   width: 100%;
   height: 100%;
   background-color: white;
}
.view_subscribe{
   width: 90%;
   margin-bottom: 8px;
   height: 60px;
   margin-left: 5%;
   margin-right: 5%;
   border-radius: 5%;
   border:1px solid rgb(153, 185, 245);
   background-color: white;
}
.view_subscribe_name{
   float: left;
   margin-left: 20px;
   font-size: 16px;
   color:rgb(92, 91, 91); 
   line-height: 60px;
   text-align: left;
   font-weight: bold;
}
.button_subscribe{
  text-align: center;
  font-size: 14px;
  color:rgb(248, 245, 245);
  margin-top: 10px;
  margin-right: 10px;
  margin-bottom: 10px;
  background-color: rgb(13, 170, 84);
  line-height: 40px;
  float: right;  
  width: auto;
}
